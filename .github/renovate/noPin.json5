{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  packageRules: [
    {
      matchDatasources: ["helmfile", "docker"],
      matchDepNames: ["spegel"],
      pinDigests: false
    },
    {
      matchDepNames: ["ghcr.io/fluxcd/flux-manifests"],
      pinDigests: false
    },
    {
      matchPackagePatterns: ["gha-runner-scale-set"],
      pinDigests: false
    },
  ],
}
